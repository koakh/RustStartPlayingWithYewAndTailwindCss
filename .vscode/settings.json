{
  "cSpell.words": [
    "Bootstrapper",
    "fullscreen",
    "icns",
    "onclick",
    "tauri"
  ],
  "tailwindCSS.includeLanguages": {
    "rust": "rs"
  },
  "editor.quickSuggestions": {
    "strings": true
  },
  "tailwindCSS.experimental.classRegex": [
    // classes!("")
    "classes!\\(\"([^\"\\)]*)",
    // r#""#
    "#\"([^\"#]*)",
  ],
  "files.watcherExclude": {
    "**/.dart_tool": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    ".flatpak/**": true,
    "_build/**": true
  },
}